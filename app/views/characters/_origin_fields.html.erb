<td class="of col-3"><%= f.input :source, collection: Origin::SOURCES, label: false %></td>
<td class="of col-3"><%= f.input :name, collection: Origin::SAMPLE, label: false %></td>
<td class="of col-4"><%= f.input :detail, label: false %></td>
